{
  "name": "nest-example",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "cert": "cd local && chmod +x get-interhyp-ca.pem.sh && source ./get-interhyp-ca.pem.sh",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_EXTRA_CA_CERTS=./local/interhyp-ca.pem nest start --watch",
    "start:debug": "cross-env NODE_EXTRA_CA_CERTS=./local/interhyp-ca.pem nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,spec}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@interhyp/nestjs-apm": "^0.4.13",
    "@interhyp/nestjs-core": "^0.9.1",
    "@interhyp/nestjs-errorhandling": "^0.4.1-snapshot-241007110839.0",
    "@interhyp/nestjs-logging": "^0.6.13",
    "@interhyp/nestjs-management": "^0.5.12",
    "@interhyp/nestjs-vault": "^1.1.1",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/platform-express": "^10.4.3",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "fast-redact": "^3.5.0",
    "mongodb-memory-server": "^10.1.2",
    "mongoose": "^8.7.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.4",
    "@types/express": "^4.17.17",
    "@types/fast-redact": "^3.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.5",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
